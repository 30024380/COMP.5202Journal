           // Asks the user to enter numbers 20 times and displays the sum
            int total = 0;

            int limit = 20;

            for (int start = 1; start <= limit; start++)
            {
                Console.WriteLine("Please Enter a Number");
                total = total + int.Parse(Console.ReadLine());
            }

            Console.WriteLine("The Sum is: " + (total));
            Console.ReadLine();
            
            // Factorial of 10
            int total = 1;
            int gg = 1;

            while (gg <= 10)
            {
                total *= gg;
                gg++;
            }

            Console.WriteLine("The Factorial of 10 is: " + (total));
            Console.ReadLine();
